   "source": [
    "# h-index problem\n",
    "\n",
    "pubs = ['A','B','C','D','E','F','G','H','I']\n",
    "cits = [1,23,6,7,8,9,6,5,5,5,5,5]\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "def hindex(cits):\n",
    "    cits.sort()\n",
    "    tot = len(cits)\n",
    "    for i in range(0,tot):\n",
    "        if tot - i < cits[i]:\n",
    "            return cits[i]\n",
    "    return 0\n",
    "print(hindex(cits))\n"
   ]
  }
